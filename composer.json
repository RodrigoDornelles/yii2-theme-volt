    "type": "yii2-extension",
    "keywords": ["dynamika", "yii2", "theme", "volt"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Rodrigo Dornelles",
            "email": "rodrigo@dynamika.com.br",
            "role": "Developer"
        }
    ],
    "support": {
